{
  "presets": [
    "@babel/preset-env",
    ["@babel/preset-react", {
      "runtime": "automatic"
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", {
      "runtime": "automatic"
    }],
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": false,
        "minify": true,
        "transpileTemplateLiterals": true
      }
    ]
  ]
}